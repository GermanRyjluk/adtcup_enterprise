{
  "expo": {
    "name": "ADT CUP Teams",
    "slug": "adtcup-teams",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "adtcupteams",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "La tua posizione è necessaria per verificare che ti trovi nel punto di partenza corretto e per monitorare i tuoi progressi durante l'evento. Non verrà condivisa per altri scopi.",
        "NSCameraUsageDescription": "L'accesso alla fotocamera è necessario per scansionare i QR code degli indovinelli e avanzare nel gioco.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.ryyuuk.adtcupteams"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#040037"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.CAMERA"],
      "package": "com.ryyuuk.adtcupteams"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#040037"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3c947803-a729-4be9-a9c5-c6f31281d839"
      }
    }
  }
}
